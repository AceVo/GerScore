    '####################################################################################################

    ReadOnly Property SelectedItem As clsPart
        Get
            Return CType(lsbParts.SelectedItem, clsPart)
        End Get
    End Property
    '####################################################################################################
    'Events
    '####################################################################################################

    Private Sub initializeList() Handles Me.Load
        Dim _type As String = "Sub"
        Dim _structname As String = Reflection.MethodBase.GetCurrentMethod.Name
        clsProgram.DebugPrefix += 1 : Debug.Print(StrDup(clsProgram.DebugPrefix, "+") & " " & "Enter in: {0} {1} ->  {2} ", _className, _type, _structname)

        lsbParts.ValueMember = "Name"
        lsbParts.DataSource = clsProjekt.Instance.Parts

        clsProgram.DebugPrefix -= 1
    End Sub

End Class